                            <select name="sensor">
                            <option value="1">Mốc mặt nền đất</option>
                            <option value="2">Lún sâu</option>
                            <option value="3">Mốc mặt nền bê tông</option>
                            <option value="4">Biến dạng khe co giãn</option>
                            <option value="5">Áp lực kẽ rỗng</option>
                            <option value="6">Áp lực mạch động</option>
                            <option value="7">Ứng suất cốt thép</option>
                            <option value="8">Cột nước</option>
                        </select>
                        </div>
                        <div class="col-xs-7 col-sm-7 col-md-7 col-lg-7">
                            <label><b>CHỌN MẶT CẮT</b></label>
                            <select name="section">
                            <option value="1">Tuyến số 1</option>
                            <option value="2">Tuyến số 2</option>
                            <option value="3">Tuyến số 3</option>
                            <option value="4">Tuyến số 4</option>
                            <option value="5">Tuyến số 5</option>
                            <option value="6">Tuyến số 6</option>
                        </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5">
                            <label><b>Từ ngày</b></label>
                            <input type="date" name="startDay">
                        </div>
                        <div class="col-xs-7 col-sm-7 col-md-7 col-lg-7">
                            <label><b>Đến ngày</b></label>
                            <input type="date" name="endDay">
                        </div>
                    </div>
                    <div class="row">
                        <label style="margin-left: 15px;margin-top: 15px;"><b>BIỂU ĐỒ</b></label>
                        <div id="chart-container" style="width: 95%; margin-left: 25px;">
                            <canvas id="chartContainer"></canvas>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header">
                                    <strong class="card-title">BẢNG DỮ LIỆU</strong>
                                </div>
                                <div class="card-body">
                                    <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>SỐ HIỆU THIẾT BỊ</th>
                                                <th>LOẠI CẢM BIẾN</th>
                                                <th>VỊ TRÍ LẮP ĐẶT</th>
                                                <th>ĐƠN VỊ ĐO</th>
                                                <th>GIÁ TRỊ HIỆN TẠI</th>
                                                <th>GIỚI HẠN TRÊN</th>
                                                <th>GIỚI HẠN DƯỚI</th>
                                                <th>TRẠNG THÁI</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>1</td>
                                                <td>5088</td>
