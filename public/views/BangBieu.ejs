<%- include('./layouts/head.ejs') %>
    <body>
        <script type="text/javascript">
            try {
                data.recordset = JSON.parse(data.recordset);
            } catch (e) {}
        </script>
        <!-- Left panel -->
        <%- include('./layouts/menu.ejs') %>
            <!-- right panel -->
            <div id="right-panel" class="right-panel">
                <%- include('./layouts/navbar.ejs') %>
                 <div class="breadcrumbs">
                    <div class="col-sm-4">
                        <div class="page-header float-left">
                            <div class="page-title">
                                <h1><%= pageTitle %></h1>
                            </div>
                        </div>
                    </div>
                </div>
            <div class="content mt-3">
                <div class="animated fadeIn">
                    <div class="row">

                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Thông số tổng quan các cảm biến</strong>
                            </div>
                            <div class="card-body">
                              <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                <thead>
                                  <tr>
                                    <th>ID</th>
                                    <th>Time</th>
                                    <th>Temp_1</th>
                                    <th>Temp_2</th>
                                    <th>Temp_3</th>
                                    <th>Temp_4</th>
                                    <th>Temp_5</th>
                                    <th>Temp_6</th>
                                    <th>Temp_7</th>
                                    <th>Temp_8</th>
                                  </tr>
                                </thead>
                                <tbody>
                                  <% for(var i=0; i < data.recordset.length; i++) { %>
                                   <tr>
                                     <td><%= data.recordset[i].ID %></td>
                                     <td><%= data.recordset[i].Time %></td>
                                     <td><%= data.recordset[i].Temp_1 %></td>
                                     <td><%= data.recordset[i].Temp_2 %></td>
                                     <td><%= data.recordset[i].Temp_3 %></td>
                                     <td><%= data.recordset[i].Temp_4 %></td>
                                     <td><%= data.recordset[i].Temp_5 %></td>
                                     <td><%= data.recordset[i].Temp_6 %></td>
                                     <td><%= data.recordset[i].Temp_7 %></td>
                                     <td><%= data.recordset[i].Temp_8 %></td>
                                   </tr>
                                  <% } %>
                                </tbody>
                              </table>
                            </div>
                        </div>
                    </div>


                    </div>
                </div><!-- .animated -->
            </div><!-- .content -->
            </div>
                    <%- include('./layouts/end.ejs') %>